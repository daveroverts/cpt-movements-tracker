    "react-dom": "18.2.0",
    "tabler-icons-react": "1.56.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
